#!/usr/bin/env perl
use warnings;
use strict;

if (scalar @ARGV < 1) {
    warn("usage: bric_password <password>\n");
    exit(1);
}

my $pwd = shift @ARGV;
my $secret = '$8fFidf*34;,a(o};"?i8J<*/#1qE3 $*23kf3K4;-+3f#\'Qz-4feI3rfe}%:e';

use Digest::MD5 qw(md5_hex);
print(md5_hex($secret . md5_hex($pwd)));
print("\n");
